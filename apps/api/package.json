{
  "name": "@nearbybazaar/api",
  "private": true,
  "version": "0.1.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "start": "node dist/index.js",
    "migrate": "ts-node src/migrations/runner.ts",
    "seed": "ts-node src/seed/index.ts",
    "seed:dev": "ts-node src/seed/index.ts dev",
    "seed:dropship": "ts-node src/seed/index.ts dropship",
    "seed:agreements": "ts-node src/seed/index.ts agreements",
    "seed:plans": "ts-node src/seed/index.ts plans",
    "seed:kaizen": "ts-node src/seed/index.ts kaizen",
    "seed:all": "ts-node src/seed/index.ts all",
    "clean": "rimraf dist",
    "test": "jest --passWithNoTests",
    "test:dropship": "jest dropship.spec.ts --passWithNoTests",
    "test:ratelimit": "jest rateLimiter.spec.ts --passWithNoTests",
    "test:seo": "jest seo.spec.ts --passWithNoTests",
    "test:sitemap": "jest sitemap.spec.ts --passWithNoTests"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@nearbybazaar/lib": "workspace:*",
    "axios": "latest",
    "bull-board": "latest",
    "bullmq": "latest",
    "cloudinary": "latest",
    "compression": "latest",
    "cors": "latest",
    "csv-stringify": "latest",
    "date-fns": "latest",
    "dotenv": "latest",
    "express": "latest",
    "fast-csv": "latest",
    "helmet": "latest",
    "ioredis": "latest",
    "jsonwebtoken": "latest",
    "meilisearch": "latest",
    "mongoose": "latest",
    "multer": "latest",
    "nodemailer": "latest",
    "swagger-jsdoc": "latest",
    "swagger-ui-express": "latest",
    "ulid": "latest",
    "xlsx": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "jest": "latest",
    "mongodb-memory-server": "latest",
    "nodemon": "latest",
    "supertest": "latest",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2"
  }
}
